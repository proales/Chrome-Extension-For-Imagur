{
   "background": {
      "persistent": false,
      "scripts": [ "js/bg.js" ]
   },
   "browser_action": {
      "default_icon": "img/ext-16x16.png"
   },
   "content_scripts": [ {
      "js": [ "js/jquery-2.1.1.min.js", "js/reddit.com.js" ],
      "matches": [ "*://*.reddit.com/*" ]
   } ],
   "description": "For Opening Imgur links in new tabs.",
   "icons": {
      "128": "img/ext-128x128.png",
      "16": "img/ext-16x16.png",
      "32": "img/ext-32x32.png",
      "48": "img/ext-48x48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Open Imgur Links",
   "permissions": [ "*://*.reddit.com/*", "tabs" ],
   "short_name": "OIL",
   "version": "1.0"
}
